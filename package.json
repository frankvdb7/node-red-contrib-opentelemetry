{
	"name": "@frankvdb/node-red-contrib-opentelemetry",
	"version": "1.0.0",
	"description": "Distributed tracing with OpenTelemetry SDK and Prometheus metrics exporter for Node-RED",
	"scripts": {
		"lint": "biome lint .",
		"format": "biome format --write .",
		"format:check": "biome format .",
		"test": "node --test"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"keywords": [
		"node-red",
		"opentelemetry",
		"tracing",
		"profiling",
		"instrumentation",
		"stats",
		"metrics",
		"prometheus"
	],
	"contributors": [
		"nioc <dev@nioc.eu>",
		"frankvdb7 <5363672+frankvdb7@users.noreply.github.com>"
	],
	"license": "LGPL-3.0-or-later",
	"homepage": "https://github.com/frankvdb7/node-red-contrib-opentelemetry",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/frankvdb7/node-red-contrib-opentelemetry.git"
	},
	"bugs": "https://github.com/frankvdb7/node-red-contrib-opentelemetry/issues",
	"node-red": {
		"version": ">=2.0.0",
		"nodes": {
			"opentelemetry": "lib/opentelemetry-node.js",
			"prometheus": "lib/prometheus-node.js"
		}
	},
	"devDependencies": {
		"@biomejs/biome": "^1.0.0"
	},
	"dependencies": {
		"@opentelemetry/api": "^1.9.0",
		"@opentelemetry/core": "^2.5.1",
		"@opentelemetry/exporter-prometheus": "^0.212.0",
		"@opentelemetry/exporter-trace-otlp-http": "^0.212.0",
		"@opentelemetry/exporter-trace-otlp-proto": "^0.212.0",
		"@opentelemetry/propagator-b3": "^2.5.1",
		"@opentelemetry/propagator-jaeger": "^2.5.1",
		"@opentelemetry/resources": "^2.5.1",
		"@opentelemetry/sdk-metrics": "^2.5.1",
		"@opentelemetry/sdk-trace-base": "^2.5.1",
		"@opentelemetry/semantic-conventions": "^1.39.0",
		"jmespath": "^0.16.0",
		"on-finished": "^2.4.1"
	}
}
